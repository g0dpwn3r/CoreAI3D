{
  "version": "0.2.1",
  "defaults": {
    "debug": {
      "workingDirectory": "D:/code/CoreAI3D",
      "stopAtEntry": false,
      "externalConsole": true
    }
  },
  "configurations": [
    {
      "type": "default",
      "project": "CmakeLists.txt",
      "projectTarget": "CoreAI3D.exe",
      "name": "Launch init",
      "args": [
        "-i",
        "gold.csv",
        "-d",
        ",",
        "-iz",
        "2",
        "-s",
        "5",
        "-e",
        "5",
        "-l",
        "5",
        "-n",
        "25",
        "--min",
        "0",
        "--max",
        "1000000",
        "-oz",
        "1",
        "-lr",
        "0.001",
        "--contains-header",
        "--start-predict",
        "--dataset-name",
        "gold-test-1A",
        "--db-host",
        "localhost",
        "--db-port",
        "33060",
        "--db-user",
        "root",
        "--db-password",
        "pmAkRV4lAAKsOfdWCUxP",
        "--db-schema",
        "CoreAI3D",
        "-di",
        "1",
        "-o",
        "gold-output.csv",
        "--language",
        "en",
        "--embedding-file",
        "emb.txt"
      ]
    },
    {
      "name": "Docker: Attach to CoreAI3D",
      "type": "docker",
      "request": "attach",
      "containerName": "coreai3d",
      "platform": "linux",
      "sourceFileMap": {
        "/app": "${workspaceFolder}"
      }
    },
    {
      "name": "Python: HuggingFace Dataset Manager",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python/huggingface_dataset_manager.py",
      "console": "integratedTerminal",
      "args": ["search", "--query", "linux"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Python: Train HuggingFace Datasets",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python/train_huggingface_datasets.py",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Python: GUI Dashboard",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/windows_gui/run_dashboard.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/windows_gui",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Python: AI Trainer (Docker)",
      "type": "docker",
      "request": "attach",
      "containerName": "ai_trainer",
      "platform": "linux",
      "sourceFileMap": {
        "/app": "${workspaceFolder}"
      }
    }
  ]
}

     